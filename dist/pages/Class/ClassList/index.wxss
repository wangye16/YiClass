.at-search-bar {
  display: flex;
  align-items: center;
  padding: 24rpx 36rpx;
  background-color: #FFF;
  overflow: hidden;
  box-sizing: border-box;
  position: relative;
  /* elements */
  /* modifiers */
}
.at-search-bar::after {
  content: "";
  position: absolute;
  transform-origin: center;
  box-sizing: border-box;
  pointer-events: none;
  top: -50%;
  left: -50%;
  right: -50%;
  bottom: -50%;
  border: 0 solid rgb(214.4, 228.4, 238.9);
  transform: scale(0.5);
  border-bottom-width: 1PX;
}
.at-search-bar__input-cnt {
  position: relative;
  flex: 1;
  width: 100%;
  height: 30PX;
  background-color: #F7F7F7;
  border-radius: 15PX;
  overflow: hidden;
  display: flex;
}
.at-search-bar__placeholder-wrap {
  display: flex;
  align-self: center;
  align-items: center;
  justify-content: center;
  padding: 0 12PX;
  height: 30PX;
  line-height: 0;
  vertical-align: middle;
  opacity: 1;
  transition: flex 0.3s;
}
.at-search-bar__placeholder-wrap .at-icon {
  color: #999;
  font-size: 14PX;
  flex-shrink: 0;
}
.at-search-bar__placeholder {
  margin-left: 12PX;
  color: #999;
  font-size: 14PX;
  flex-shrink: 0;
}
.at-search-bar__input {
  position: absolute;
  display: block;
  top: 0;
  left: 0;
  width: 100%;
  height: 30PX !important;
  padding: 0 37PX;
  color: #000;
  font-size: 14PX !important;
  text-align: left;
  background-color: transparent;
  transition: width 0.3s;
  box-sizing: border-box;
}
.at-search-bar__clear {
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: center;
  top: 0;
  right: 12PX;
  height: 30PX;
  width: 25PX;
  color: #999;
  font-size: 14PX;
  line-height: 0;
  vertical-align: middle;
}
.at-search-bar__action {
  flex: none;
  display: block;
  margin-left: 10PX;
  padding: 0 10PX;
  height: 30PX;
  color: #FFF;
  font-size: 14PX;
  line-height: 30PX;
  border-radius: 4PX;
  background-color: #6190E8;
  transition: margin-right 0.3s, opacity 0.3s;
  opacity: 0;
}
.at-search-bar--fixed {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 800;
}
.class-container {
  font-family: Roboto;
}
.class-container .search-container {
  width: 100%;
  height: 100rpx;
  position: sticky;
  top: 0;
  background-color: #fff;
}
.class-container .search-input {
  margin: 40rpx auto;
  padding-left: 40rpx;
  width: 694rpx;
  height: 64rpx;
  background: #F3F4F6;
  border-radius: 32rpx;
  -webkit-border-radius: 32rpx;
  -moz-border-radius: 32rpx;
  -ms-border-radius: 32rpx;
  -o-border-radius: 32rpx;
}
.class-container .search-input-placeholder {
  display: inline-block;
  margin: 0 auto;
  color: #6B7280;
}
